<script>
  document.addEventListener('DOMContentLoaded', () => {
    const wrapper = document.querySelector('[data-marquee-wrapper]');
    if (!wrapper) return;
    const track = wrapper.querySelector('[data-marquee-track]');
    if (!(track instanceof HTMLElement)) return;

    // Build a second row with reverse marquee and same duration
    const row = document.createElement('div');
    row.className = 'flex items-center mt-4';

    // Clone the existing track to preserve Astro's scoped attributes
    const newTrack = track.cloneNode(true) as HTMLElement;
    newTrack.classList.remove('animate-marquee-fast');
    newTrack.classList.add('animate-marquee-reverse');

    row.appendChild(newTrack);
    wrapper.appendChild(row);
  });
</script>

